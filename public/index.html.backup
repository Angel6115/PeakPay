      .hero{ grid-template-columns: 1.2fr 1fr } 
    }
    .hero h2{ 
      margin:0 0 20px; 
      font-size:clamp(32px,5vw,48px); 
      line-height:1.1; 
      font-weight: 900;
      color: var(--text);
    }
    .hero p{ 
      margin:0 0 28px; 
      color:var(--muted); 
      font-size:clamp(16px,2.6vw,20px);
      line-height: 1.6;
    }
    .cta-row{ 
      display:flex; 
      gap:16px; 
      flex-wrap:wrap;
    }
    
    /* Preview */
    .preview-block{ 
      position:relative; 
      width:100%; 
      max-width:420px;
      height:auto;
      margin:0 auto;
    }
    .preview-img{ 
      width:100%; 
      aspect-ratio:4/3; 
      border-radius:20px; 
      object-fit:cover; 
      filter:blur(20px) brightness(0.9); 
      box-shadow:0 10px 30px rgba(102,126,234,.2);
    }
    .mosaic{ 
      position:absolute; 
      top:0; 
      left:0; 
      width:100%; 
      height:100%; 
      display:grid; 
      grid-template-columns:repeat(4,1fr); 
      grid-template-rows:repeat(3,1fr); 
      gap:6px; 
      padding:10px;
    }
    .tile{ 
      border-radius:10px; 
      background: linear-gradient(135deg,#cbd5e1 0%,#94a3b8 100%);
      filter: blur(8px) brightness(1.1) opacity(0.75); 
      box-shadow: 0 4px 10px rgba(79,70,229,.15);
    }
    .tile.free{ 
      filter:none; 
      background: linear-gradient(135deg,#10b981 0%,#059669 100%);
      animation: pulse 2s infinite;
    }
    @keyframes pulse {
      0%, 100% { transform: scale(1); }
      50% { transform: scale(1.05); }
    }
    .preview-label{ 
      position:absolute; 
      left:0; 
      right:0; 
      bottom:8px; 
      text-align:center; 
      color:#fff; 
      font-size:16px; 
      font-weight:700; 
      background:rgba(102,126,234,0.9); 
      padding:8px 0;
      border-radius: 0 0 20px 20px;
    }
    
    /* How It Works with Slider */
