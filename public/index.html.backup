    .grid-demo{
      display: grid;
      grid-template-columns: repeat(4, 1fr);
      gap: 8px;
      max-width: 340px;
      margin: 0 auto;
      aspect-ratio: 1;
    }
    .tile-demo{
      border-radius: 10px;
      position: relative;
      overflow: hidden;
      background: #cbd5e1;
      display: flex;
      align-items: center;
      justify-content: center;
      font-size: 13px;
      font-weight: 700;
    }
    .tile-demo.blur{
      filter: blur(12px);
      background: linear-gradient(135deg, #94a3b8, #cbd5e1);
    }
    .tile-demo.free{
      background: linear-gradient(135deg, #10b981, #059669);
      color: white;
      animation: pulse 2s infinite;
    }
    .tile-demo.locked{
      background: linear-gradient(135deg, #667eea, #818cf8);
      color: white;
      font-size: 11px;
    }
    .tile-demo.revealed{
      background: linear-gradient(135deg, #f59e0b, #f97316);
      color: white;
      animation: reveal 0.5s ease-out;
