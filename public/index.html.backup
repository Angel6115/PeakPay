      margin-top: 40px;
    }
    .cats-section h3{
      font-size: 32px;
      margin-bottom: 12px;
      font-weight: 800;
    }
    .cats-section .subtitle{
      color: var(--muted);
      font-size: 16px;
      margin-bottom: 28px;
    }
    .cats{ 
      display:grid; 
      gap:16px;
    }
    @media (min-width:640px){ 
      .cats{ grid-template-columns: repeat(2, 1fr); } 
    }
    @media (min-width:900px){ 
      .cats{ grid-template-columns: repeat(3, 1fr); } 
    }
    .cat{ 
      display:flex;
      align-items: center;
      gap: 16px;
      padding:24px;
      border-radius:16px; 
      text-decoration:none; 
      color:var(--text); 
      font-weight:600; 
      font-size:17px;
      background: var(--card);
      border:2px solid var(--ring);
      transition: all 0.3s;
      cursor:pointer;
    }
    .cat:hover{
      transform: translateY(-3px);
      box-shadow: 0 8px 20px rgba(102,126,234,.12);
      border-color: var(--accent);
    }
    .cat-color{
      width: 50px;
