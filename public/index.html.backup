<script>
  // Banner functionality
  const banner = document.getElementById('earlyBanner');
  const closeBtn = document.getElementById('closeBanner');
  
  closeBtn.addEventListener('click', () => {
    banner.classList.add('hidden');
    localStorage.setItem('peekpay_banner_closed', 'true');
  });
  
  if (localStorage.getItem('peekpay_banner_closed') === 'true') {
    banner.classList.add('hidden');
  }
  
  // Sync spots
  const bannerSpots = document.getElementById('bannerSpots');
  const savedSpots = localStorage.getItem('peekpay_spots');
  if (savedSpots) {
    bannerSpots.textContent = savedSpots;
  }
  
  setInterval(() => {
    const currentSpots = localStorage.getItem('peekpay_spots');
    if (currentSpots) {
      bannerSpots.textContent = currentSpots;
    }
  }, 5000);
  
  // Slider functionality
  let currentSlide = 0;
  const slider = document.getElementById('slider');
  const dots = document.querySelectorAll('.dot');
  const totalSlides = 3;
  
  function goToSlide(n) {
    currentSlide = n;
    slider.style.transform = `translateX(-${currentSlide * 100}%)`;
    dots.forEach((dot, i) => {
      dot.classList.toggle('active', i === currentSlide);
    });
  }
  
  function nextSlide() {
    currentSlide = (currentSlide + 1) % totalSlides;
    goToSlide(currentSlide);
  }
  
  let autoPlay = setInterval(nextSlide, 4000);
  
  dots.forEach(dot => {
    dot.addEventListener('click', (e) => {
      clearInterval(autoPlay);
      goToSlide(parseInt(e.target.dataset.slide));
      autoPlay = setInterval(nextSlide, 4000);
    });
  });
</script>
